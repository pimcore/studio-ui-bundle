{# @var bundleCssFiles string[] #}
{# @var bundleJsFiles string[] #}
<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8">

        {% for cssFile in bundleCssFiles %}
            <link rel="stylesheet" href="{{ cssFile }}"/>
        {% endfor %}
    </head>

    <body>
        <div id="app"></div>

        {% for jsFile in bundleJsFiles %}
            <script src="{{ jsFile }}"></script>
        {% endfor %}
    </body>
</html>
